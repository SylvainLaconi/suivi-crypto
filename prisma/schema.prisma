generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DATABASE_URL")
}

model Crypto {
  id        String   @id @default(uuid())
  name      String
  symbol    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
